  "description": "Native node module for detecting global window creation/destruction",
  "main": "index.js",
  "devDependencies": {
    "lodash": "^4.13.1",
    "nan": "^2.4.0",
    "node-gyp": "^1.0.3",
    "node-pre-gyp": "^0.6.19"
  },
  "dependencies": {
    "async": "^1.5.2",
    "bindings": "^1.2.1",
    "node-inspector": "^0.12.5"
  },
  "scripts": {
    "test": "./node_modules/.bin/jasmine-node spec",
